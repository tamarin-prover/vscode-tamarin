{
  "name": "tamarin-prover",
  "version": "1.1.6",
  "engines": {
    "vscode": "^1.90.0"
  },
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/tamarin-prover/vscode-tamarin.git"
  },
  "publisher": "tamarin-prover",
  "categories": [
    "Programming Languages",
    "Snippets",
    "Extension Packs"
  ],
  "activationEvents": [],
  "contributes": {
    "languages": [
      {
        "id": "tamarin",
        "aliases": [
          "tamarin"
        ],
        "extensions": [
          ".spthy",
          ".splib"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tamarin",
        "scopeName": "source.tamarin",
        "path": "./syntaxes/tamarin.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "tamarin",
        "path": "./snippets/snippets.json"
      }
    ],
    "commands": [
      {
        "command": "tamarin.renameCodeAction",
        "when": "editorLangId == tamarin",
        "title": "Rename"
      },
      {
        "command": "Rename",
        "title": "Rename",
        "when": "editorLangId == tamarin"
      },
      {
        "command": "Search definition",
        "title": "Search definition",
        "when": "editorLangId == tamarin"
      },
      {
        "command": "extension.tabKeyBinding",
        "title": "Navigate to next symbol",
        "when": "editorLangId == tamarin"
      },
      {
        "command": "myExtension.navigateToNextSymbol",
        "title": "Navigate to Next Symbol",
        "when": "editorLangId == tamarin"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+alt+n",
        "command": "extension.tabKeyBinding",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "Tamarin",
      "properties": {}
    },
    "menus": {
      "editor/context": [
        {
          "command": "Rename",
          "group": "navigation",
          "when": "editorTextFocus && editorLangId == tamarin && !inDebugRepl"
        },
        {
          "command": "Search definition",
          "group": "navigation",
          "when": "editorTextFocus && editorLangId == tamarin && !inDebugRepl"
        }
      ]
    }
  },
  "main": "./out/extension.js",
  "types": "bindings/node",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && copyfiles -u 1 src/grammar/**/* out/",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "prebuildify": "prebuildify --napi --strip"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.90.0",
    "copyfiles": "^2.4.1",
    "prebuildify": "^6.0.0",
    "typescript": "^5.4.5",
    "ovsx": "^0.8.1",
    "vsce": "^2.15.0",
    "tree-sitter-cli": "^0.22.6",
    "tree-sitter-javascript": "^0.21.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "node-gyp-build": "^4.8.0",
    "tree-sitter": "^0.21.0",
    "tree-sitter-highlight": "^1.0.1",
    "vscode-languageclient": "^9.0.1",
    "web-tree-sitter": "^0.22.6"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": false
    }
  },
  "license": "MIT",
  "tree-sitter": [
    {
      "scope": "source.spthy",
      "injection-regex": "^spthy$"
    }
  ]
}
